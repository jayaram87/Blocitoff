<% if user == current_user %>
    <% user.items.each do |item| %>
        <table>
            <tr>
                <td><%= item.name %></td>
                <td><small> <div class="time"> created at <%= time_ago_in_words(item.created_at) %></small></div></td>
                <td> <div class="item"><%= link_to "", [user, item], method: :delete, remote: true, class: 'glyphicon glyphicon-ok' %> </div></td>
            </tr>
        </table>
    <% end %>
<% end %>